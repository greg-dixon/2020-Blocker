{
    "manifest_version": 2,
    "name": "2020 blocker",
    "version": "1.0",
    "description": "The 2020 blocker is made for people who can't even anymore.",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["./main.js"]
    }],
    "browser_action": {
        "default_icon": "off.png",
        "default_title": "2020 Blocker",
        "default_popup": "options.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    }


}


